<tr id = "post_{{id}}">
  <td>
    <div class="layui-btn-group">
      <button class="layui-btn layui-btn-sm" onclick='del_post("{{id}}")'>
        <i class="layui-icon">&#xe640;</i>
      </button>
    </div>
  </td>
  <td>{{create | moment 'YYYY-MM-DD hh:mm:ss'}}</td>
  <td>
    <a href="/admin/post/{{id}}">{{title}}</a>
  </td>
</tr>