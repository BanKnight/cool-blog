<div class="post">
    <h1 class="post-title">
        <a href="/post/{{id}}">
            {{title}}
        </a>
    </h1>
    <div class="post-meta">
        {{create | moment 'YYYY-MM-DD'}}
    </div>
    <div class="post-content">
        {{@summary}}
    </div>
    <p class="readmore">
        <a href="/post/{{id}}">
            阅读全文
        </a>
    </p>
</div>